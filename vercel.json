{
  "buildCommand": "cd frontend-fhe-race && npm install && npm run build",
  "outputDirectory": "frontend-fhe-race/build",
  "installCommand": "npm install --prefix frontend-fhe-race",
  "framework": "create-react-app",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        },
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        },
        {
          "key": "Cross-Origin-Resource-Policy",
          "value": "cross-origin"
        }
      ]
    }
  ]
}

